<div class="row userProfilePadding">
    <div class="col-lg-12">
        <div class="row">
            <div class="col-lg-4">
                <table class="tableBorder">
                    <thead class="tableHead">
                        <th class="tableText"> Available Functions </th>
                    </thead>
                    <tbody>
                        <tr class="tableBorder" ng-show="vm.userAvailableFunctionsNames.length > 0" ng-repeat="UserAvailableFunctions in vm.userAvailableFunctionsNames track by $index">
                            <td class="tableText">
                                {{UserAvailableFunctions}}
                            </td>
                        </tr>
                        
                        <tr ng-show="vm.userAvailableFunctionsNames.length== 0">
                            <td  class="tableErrorText">
                                No Function Available..! 
                            </td>
                        </tr>
                    </tbody>

                </table>
            </div>
            <div class="col-lg-1"></div>
            <div class="col-lg-4">
                <table class="tableBorder">
                    <thead class="tableHead">
                        <th class="tableText"> Excluded Functions </th>
                    </thead>
                    <tbody>
                        <tr class="tableBorder" ng-show="vm.userExcludedFunctionsNames.length > 0" ng-repeat="UserExcludedFunctions in vm.userExcludedFunctionsNames track by $index">
                            <td class="tableText">
                                {{UserExcludedFunctions}}
                            </td>
                        </tr>
                        
                        <tr ng-show="vm.userExcludedFunctionsNames.length== 0">
                            <td  class="tableErrorText">
                                No Function Excluded..! 
                            </td>
                        </tr>
                    </tbody>

                </table>
            </div>
            <div class="col-lg-4"></div>
        </div>
    </div>
</div>
