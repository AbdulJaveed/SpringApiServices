<div class="form-group row">
		<div ng-show="vm.preview.content != null" class=" col-md-10 col-sm-12"> 
				<img ngf-src="vm.preview.content" class="thumb img-width"></div>
		<div class="col-md-5 col-sm-12 text-align-center  col-form-label basicTemplate" ng-show="vm.preview.content == null"><img class="thumb img-width" src="img/upload.png"/></div>
</div>
<div class="form-group row">
		<label class="col-md-10 col-sm-12 text-align-center  col-form-label basicTemplate"
		for="empName"> {{vm.basicInfo.fullName}}</label>
</div>
<div class="form-group row">
		<label class="col-md-10 col-sm-12 text-align-center  col-form-label basicTemplate"
		for="employeeNumber"> {{vm.basicInfo.employeeNumber}} </label>
</div>
<div class="form-group row">
		<label class="col-md-10 col-sm-12 text-align-center  col-form-label basicTemplate"
		for="orgName"> {{vm.basicInfo.orgName}} </label>
</div>
